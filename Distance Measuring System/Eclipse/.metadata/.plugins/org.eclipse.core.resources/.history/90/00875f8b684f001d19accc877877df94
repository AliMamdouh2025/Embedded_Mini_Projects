#include "lcd.h"

int main(void)
{
	LCD_init(); /* Initialize the LCD */
	LCD_displayString("LCD 4-Bits Mode");
	LCD_displayStringRowColumn(1, 0, "MT Emb. Diploma");

	while(1)
	{
		/* Note: we write the code before while(1) because we want to execute it only once */
	}

}

